{
    "app": {
        "name": "InventoryHub",
        "seedEnabled": true,
        "seedCount": 50,
        "fakerMappings": {
            "carrier": "randomElement([\"FedEx\", \"UPS\", \"DHL\", \"USPS\"])"
        }
    },
    "entities": [
        {
            "name": "User",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "name",
                    "type": "string",
                    "sortable": true,
                    "searchable": true
                },
                {
                    "name": "email",
                    "type": "string",
                    "unique": true,
                    "sortable": true,
                    "searchable": true
                },
                {
                    "name": "password",
                    "type": "password",
                    "hidden": true
                },
                {
                    "name": "settings",
                    "type": "json",
                    "nullable": true
                },
                {
                    "name": "active",
                    "type": "boolean",
                    "default": true
                }
            ],
            "relations": [
                {
                    "type": "hasMany",
                    "relatedEntityName": "Order",
                    "as": "orders"
                },
                {
                    "type": "hasMany",
                    "relatedEntityName": "Shipment",
                    "as": "shipments"
                },
                {
                    "type": "hasMany",
                    "relatedEntityName": "Warehouse",
                    "as": "warehouses"
                },
                {
                    "type": "belongsToMany",
                    "relatedEntityName": "Role",
                    "pivot": "role_user"
                }
            ],
            "filters": [
                {
                    "field": "name",
                    "type": "text"
                },
                {
                    "field": "email",
                    "type": "text"
                },
                {
                    "field": "active",
                    "type": "boolean"
                }
            ],
            "settings": {
                "has_images": true
            }
        },
        {
            "name": "Role",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "name",
                    "type": "string",
                    "unique": true
                }
            ],
            "relations": [
                {
                    "type": "belongsToMany",
                    "relatedEntityName": "User",
                    "pivot": "role_user"
                }
            ]
        },
        {
            "name": "Warehouse",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "user_id",
                    "type": "unsignedBigInteger"
                },
                {
                    "name": "name",
                    "type": "string",
                    "searchable": true
                },
                {
                    "name": "location",
                    "type": "string"
                },
                {
                    "name": "capacity",
                    "type": "integer",
                    "min": 0,
                    "max": 200
                },
                {
                    "name": "metadata",
                    "type": "json",
                    "nullable": true
                }
            ],
            "relations": [
                {
                    "type": "belongsTo",
                    "relatedEntityName": "User",
                    "as": "manager",
                    "foreignKey": "user_id"
                },
                {
                    "type": "hasMany",
                    "relatedEntityName": "Product",
                    "as": "products"
                }
            ]
        },
        {
            "name": "Category",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "name",
                    "type": "string",
                    "searchable": true
                },
                {
                    "name": "description",
                    "type": "text",
                    "nullable": true
                }
            ],
            "relations": [
                {
                    "type": "belongsToMany",
                    "relatedEntityName": "Product",
                    "pivot": "category_product"
                }
            ]
        },
        {
            "name": "Product",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "warehouse_id",
                    "type": "unsignedBigInteger"
                },
                {
                    "name": "name",
                    "type": "string",
                    "searchable": true,
                    "sortable": true
                },
                {
                    "name": "sku",
                    "type": "string",
                    "unique": true
                },
                {
                    "name": "price",
                    "type": "decimal",
                    "min": 0,
                    "max": 10000,
                    "sortable": true
                },
                {
                    "name": "weight",
                    "type": "float",
                    "nullable": true,
                    "min": 0,
                    "max": 100,
                    "sortable": true
                },
                {
                    "name": "available_from",
                    "type": "date",
                    "nullable": true,
                    "sortable": true
                },
                {
                    "name": "expires_at",
                    "type": "dateTime",
                    "nullable": true,
                    "sortable": true
                },
                {
                    "name": "status",
                    "type": "enum",
                    "values": [
                        "active",
                        "inactive",
                        "discontinued"
                    ],
                    "sortable": true
                }
            ],
            "relations": [
                {
                    "type": "belongsTo",
                    "relatedEntityName": "Warehouse",
                    "foreignKey": "warehouse_id"
                },
                {
                    "type": "belongsToMany",
                    "relatedEntityName": "Category",
                    "pivot": "category_product"
                }
            ],
            "filters": [
                {
                    "field": "name",
                    "type": "text"
                },
                {
                    "field": "status",
                    "type": "enum"
                },
                {
                    "field": "price",
                    "type": "range"
                },
                {
                    "field": "available_from",
                    "type": "date_range"
                }
            ],
            "settings": {
                "has_images": true
            }
        },
        {
            "name": "Order",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "user_id",
                    "type": "unsignedBigInteger"
                },
                {
                    "name": "order_number",
                    "type": "string",
                    "unique": true
                },
                {
                    "name": "status",
                    "type": "enum",
                    "values": [
                        "pending",
                        "processing",
                        "shipped",
                        "delivered"
                    ]
                },
                {
                    "name": "total",
                    "type": "decimal",
                    "min": 0,
                    "max": 10000
                },
                {
                    "name": "placed_at",
                    "type": "dateTime"
                }
            ],
            "relations": [
                {
                    "type": "belongsTo",
                    "relatedEntityName": "User",
                    "foreignKey": "user_id"
                },
                {
                    "type": "hasMany",
                    "relatedEntityName": "OrderItem",
                    "as": "items"
                },
                {
                    "type": "hasOne",
                    "relatedEntityName": "Shipment",
                    "as": "shipment"
                }
            ]
        },
        {
            "name": "OrderItem",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "order_id",
                    "type": "unsignedBigInteger"
                },
                {
                    "name": "product_id",
                    "type": "unsignedBigInteger"
                },
                {
                    "name": "quantity",
                    "type": "integer",
                    "min": 1,
                    "max": 10
                },
                {
                    "name": "unit_price",
                    "type": "decimal",
                    "min": 0,
                    "max": 10000
                }
            ],
            "relations": [
                {
                    "type": "belongsTo",
                    "relatedEntityName": "Order",
                    "foreignKey": "order_id"
                },
                {
                    "type": "belongsTo",
                    "relatedEntityName": "Product",
                    "foreignKey": "product_id"
                }
            ]
        },
        {
            "name": "Shipment",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "order_id",
                    "type": "unsignedBigInteger"
                },
                {
                    "name": "user_id",
                    "type": "unsignedBigInteger"
                },
                {
                    "name": "shipped_at",
                    "type": "dateTime",
                    "nullable": true
                },
                {
                    "name": "delivered_at",
                    "type": "dateTime",
                    "nullable": true
                },
                {
                    "name": "carrier",
                    "type": "string",
                    "nullable": true
                }
            ],
            "relations": [
                {
                    "type": "belongsTo",
                    "relatedEntityName": "User",
                    "foreignKey": "user_id"
                },
                {
                    "type": "belongsTo",
                    "relatedEntityName": "Order",
                    "foreignKey": "order_id"
                },
                {
                    "type": "hasOne",
                    "relatedEntityName": "CarrierDetail",
                    "as": "carrierDetail"
                }
            ]
        },
        {
            "name": "CarrierDetail",
            "fields": [
                {
                    "name": "id",
                    "type": "id"
                },
                {
                    "name": "shipment_id",
                    "type": "unsignedBigInteger"
                },
                {
                    "name": "tracking_number",
                    "type": "string",
                    "nullable": true
                },
                {
                    "name": "service_level",
                    "type": "string",
                    "nullable": true
                }
            ],
            "relations": [
                {
                    "type": "belongsTo",
                    "relatedEntityName": "Shipment",
                    "foreignKey": "shipment_id"
                }
            ]
        }
    ]
}
